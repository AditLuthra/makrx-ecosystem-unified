{
  "realm": "makrx",
  "displayName": "MakrX Ecosystem",
  "enabled": true,
  "sslRequired": "external",
  "registrationAllowed": true,
  "loginWithEmailAllowed": true,
  "duplicateEmailsAllowed": false,
  "resetPasswordAllowed": true,
  "editUsernameAllowed": false,
  "bruteForceProtected": true,
  "permanentLockout": false,
  "maxFailureWaitSeconds": 900,
  "minimumQuickLoginWaitSeconds": 60,
  "waitIncrementSeconds": 60,
  "quickLoginCheckMilliSeconds": 1000,
  "maxDeltaTimeSeconds": 43200,
  "failureFactor": 30,
  "accessTokenLifespan": 300,
  "accessTokenLifespanForImplicitFlow": 900,
  "ssoSessionIdleTimeout": 1800,
  "ssoSessionMaxLifespan": 36000,
  "offlineSessionIdleTimeout": 2592000,
  "offlineSessionMaxLifespan": 5184000,
  "roles": {
    "realm": [
      {
        "name": "user",
        "description": "Standard user role",
        "composite": false,
        "clientRole": false,
        "containerId": "makrx"
      },
      {
        "name": "admin",
        "description": "Administrator role",
        "composite": false,
        "clientRole": false,
        "containerId": "makrx"
      },
      {
        "name": "makerspace_owner",
        "description": "Makerspace owner role",
        "composite": false,
        "clientRole": false,
        "containerId": "makrx"
      },
      {
        "name": "event_organizer",
        "description": "Event organizer role",
        "composite": false,
        "clientRole": false,
        "containerId": "makrx"
      },
      {
        "name": "store_manager",
        "description": "Store manager role",
        "composite": false,
        "clientRole": false,
        "containerId": "makrx"
      }
    ]
  },
  "clients": [
    {
      "clientId": "makrx-gateway-frontend",
      "name": "MakrX Gateway Frontend",
      "enabled": true,
      "redirectUris": ["http://localhost:3000/*", "https://makrx.org/*"],
      "webOrigins": ["http://localhost:3000", "https://makrx.org"],
      "protocol": "openid-connect",
      "publicClient": true,
      "standardFlowEnabled": true,
      "implicitFlowEnabled": false,
      "directAccessGrantsEnabled": false,
      "serviceAccountsEnabled": false,
      "attributes": {
        "pkce.code.challenge.method": "S256"
      }
    },
    {
      "clientId": "makrx-gateway-frontend-hacker",
      "name": "MakrX Gateway Frontend Hacker",
      "enabled": true,
      "redirectUris": ["http://localhost:3001/*", "https://hacker.makrx.org/*"],
      "webOrigins": ["http://localhost:3001", "https://hacker.makrx.org"],
      "protocol": "openid-connect",
      "publicClient": true,
      "standardFlowEnabled": true,
      "implicitFlowEnabled": false,
      "directAccessGrantsEnabled": false,
      "serviceAccountsEnabled": false,
      "attributes": {
        "pkce.code.challenge.method": "S256"
      }
    },
    {
      "clientId": "makrx-makrcave",
      "name": "MakrCave",
      "enabled": true,
      "redirectUris": ["http://localhost:3002/*", "https://makrcave.com/*"],
      "webOrigins": ["http://localhost:3002", "https://makrcave.com"],
      "protocol": "openid-connect",
      "publicClient": true,
      "standardFlowEnabled": true,
      "implicitFlowEnabled": false,
      "directAccessGrantsEnabled": false,
      "serviceAccountsEnabled": false,
      "protocolMappers": [
        {
          "name": "aud: makrcave-api",
          "protocol": "openid-connect",
          "protocolMapper": "oidc-audience-mapper",
          "config": {
            "included.client.audience": "makrcave-api",
            "id.token.claim": "true",
            "access.token.claim": "true"
          }
        }
      ],
      "attributes": {
        "pkce.code.challenge.method": "S256"
      }
    },
    {
      "clientId": "makrx-store",
      "name": "MakrX Store",
      "enabled": true,
      "redirectUris": ["http://localhost:3003/*", "https://makrx.store/*"],
      "webOrigins": ["http://localhost:3003", "https://makrx.store"],
      "protocol": "openid-connect",
      "publicClient": true,
      "standardFlowEnabled": true,
      "implicitFlowEnabled": false,
      "directAccessGrantsEnabled": false,
      "serviceAccountsEnabled": false,
      "protocolMappers": [
        {
          "name": "aud: makrx-store-api",
          "protocol": "openid-connect",
          "protocolMapper": "oidc-audience-mapper",
          "config": {
            "included.client.audience": "makrx-store-api",
            "id.token.claim": "true",
            "access.token.claim": "true"
          }
        }
      ],
      "attributes": {
        "pkce.code.challenge.method": "S256"
      }
    },
    {
      "clientId": "makrx-events",
      "name": "MakrX Events",
      "enabled": true,
      "redirectUris": ["http://localhost:3004/*", "https://staging.makrx.events/*"],
      "webOrigins": ["http://localhost:3004", "https://staging.makrx.events"],
      "protocol": "openid-connect",
      "publicClient": true,
      "standardFlowEnabled": true,
      "implicitFlowEnabled": false,
      "directAccessGrantsEnabled": false,
      "serviceAccountsEnabled": false,
      "protocolMappers": [
        {
          "name": "aud: makrx-events-api",
          "protocol": "openid-connect",
          "protocolMapper": "oidc-audience-mapper",
          "config": {
            "included.client.audience": "makrx-events-api",
            "id.token.claim": "true",
            "access.token.claim": "true"
          }
        }
      ],
      "attributes": {
        "pkce.code.challenge.method": "S256"
      }
    },
    {
      "clientId": "makrcave-api",
      "name": "MakrCave API",
      "enabled": true,
      "clientAuthenticatorType": "client-secret",
      "protocol": "openid-connect",
      "serviceAccountsEnabled": true
    },
    {
      "clientId": "makrx-store-api",
      "name": "MakrX Store API",
      "enabled": true,
      "clientAuthenticatorType": "client-secret",
      "protocol": "openid-connect",
      "serviceAccountsEnabled": true
    },
    {
      "clientId": "makrx-events-api",
      "name": "MakrX Events API",
      "enabled": true,
      "clientAuthenticatorType": "client-secret",
      "protocol": "openid-connect",
      "serviceAccountsEnabled": true
    }
  ],
  
  "smtpServer": {},
  "loginTheme": "keycloak",
  "accountTheme": "keycloak.v2",
  "adminTheme": "keycloak",
  "emailTheme": "keycloak",
  "internationalizationEnabled": true,
  "supportedLocales": ["en", "es", "fr", "de"],
  "defaultLocale": "en"
}
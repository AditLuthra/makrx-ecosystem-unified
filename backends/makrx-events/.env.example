# Example environment for makrx-events backend (safe defaults)
# Copy to .env and adjust for your environment. Do not commit real secrets.

# Database Configuration
DATABASE_URL=postgresql://makrx:makrx_dev_password@localhost:5433/makrx_ecosystem

# Redis Configuration
REDIS_URL=redis://localhost:6380

# Keycloak Configuration
KEYCLOAK_URL=http://localhost:8081
KEYCLOAK_REALM=makrx
KEYCLOAK_CLIENT_ID=makrx-events

# Application Configuration
PORT=8002
ENVIRONMENT=development

# Security
SECRET_KEY=dev-secret-key-change-in-production

